#!/Users/niels/coding-challenges/energy-hub-challenge/venv/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'Replay','console_scripts','replay'
__requires__ = 'Replay'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Replay', 'console_scripts', 'replay')()
    )
